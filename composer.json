{
    "name": "kyleatdnd/gridpane-laravel",
    "type": "library",
    "description": "Laravel wrapper for kyleatdnd/gridpane-api-client-php package",
    "minimum-stability": "stable",
    "keywords": [
        "gridpane",
        "laravel"
    ],
    "homepage": "http://designanddevelop.io",
    "license": "MIT",
    "authors": [
        {
            "name": "KyleAtDND",
            "email": "Wesley@DesignAndDevelop.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0",
        "kyleatdnd/gridpane-api-client-php": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KyleAtDND\\Gridpane\\Providers\\GridpaneServiceProvider"
            ],
            "aliases": {
                "Gridpane": "KyleAtDND\\Gridpane\\Facades\\Gridpane"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "KyleAtDND\\Gridpane\\": "src"
        }
    }
}
