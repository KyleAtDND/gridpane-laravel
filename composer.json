{
    "name": "kylewlawrence/gridpane-laravel",
    "type": "library",
    "description": "Laravel wrapper for kylewlawrence/gridpane-api-client-php package",
    "minimum-stability": "stable",
    "keywords": [
        "gridpane",
        "laravel"
    ],
    "homepage": "http://designanddevelop.io",
    "license": "MIT",
    "authors": [
        {
            "name": "KyleWLawrence",
            "email": "Kyle@DesignAndDevelop.io"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": ">=8.0",
        "kylewlawrence/gridpane-api-client-php": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KyleWLawrence\\GridPane\\Providers\\GridPaneServiceProvider"
            ],
            "aliases": {
                "GridPane": "KyleWLawrence\\GridPane\\Facades\\GridPane"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "KyleWLawrence\\GridPane\\": "src"
        }
    }
}
